{
  "compileOnSave": false,
  "compilerOptions": {
    "esModuleInterop": true,
    "baseUrl": "./",
    "declaration": false,
    "experimentalDecorators": true,
    "importHelpers": true,
    "module": "es2020",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "./dist/out-tsc",
    "skipLibCheck": true,
    "sourceMap": true,
    "stripInternal": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@typed-mxgraph",
      "typings"
    ],

    "lib": [
      "es2017",
      "es2015.collection",
      "es2015.iterable",
      "dom"
    ]
  }
}
